public class tabel_perkalian
{
  public static void main(String[]args)
  {
    int a,b,c;
   
    
    for (a=1;a<=9;a++)
    {
      for(b=1;b<=9;b++)
      {
        c=a*b;
        System.out.print(b+"x"+ a +"="+ c+"\t");
        if (b>=9)
        {
          System.out.println();
          
        }
      }
    }    
  }
}
